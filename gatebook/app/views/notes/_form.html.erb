<%= form_for(@note) do |f|%>
<% if @note.errors.any? %>
<div id="error_explanation" class="alert alert-danger">
    <h2><%= @note.errors.count %>件のエラーがあります。</h2>
    <ul>
        <% @note.errors.full_messages.each do |message| %>
        <li>
            <%= message %>
        </li>
        <% end %>
    </ul>
</div>
<% end %>

ユーザID
<%= f.collection_select :user_id, User.all, :id, :id %>
タイトル
<%= f.text_field :title, class: "form-control title-field" %>
内容
<%= f.text_area :content, class: "form-tontrol content-field" %>
<%= f.submit "",class:"btn primary-btn" %>
<% end %>